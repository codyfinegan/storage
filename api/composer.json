{
    "name": "codyfinegan/storage-api",
    "description": "API written in Slim for Storage",
    "homepage": "https://github.com/codyfinegan/storage",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "doctrine/dbal": "^3.3",
        "monolog/monolog": "^2.3",
        "php-di/php-di": "^6.3",
        "php-di/slim-bridge": "^3.2",
        "slim/psr7": "^1.5",
        "slim/slim": "^4.9",
        "vlucas/phpdotenv": "^5.4",
        "webonyx/graphql-php": "^14.11"
    },
    "require-dev": {
        "doctrine/migrations": "^3.5",
        "jangregor/phpstan-prophecy": "^1.0.0",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpstan/extension-installer": "^1.1.0",
        "phpstan/phpstan": "^1.3",
        "phpunit/phpunit": "^9.5.11",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "StorageApi\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
